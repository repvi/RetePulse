        "react-port": 3000,
        "api-url": "http://localhost:5000",
        "socket-url": "http://localhost:5000"
    },
    
    "backend": {
        "flask": {
            "host": "localhost",
            "port": 5000,
            "debug": true,
            "secret-key": "supersecretkey"
